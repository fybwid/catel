<h1>Membuat Surat Keluar</h1>

<%= form_for :surat_keluar, url: surat_keluar_path do |f| %>
  <p>
    <%= f.label :keterangan %><br>
    <%= f.text_field :keterangan %>
  </p>
 
  <p>
    <%= f.label :lampiran %><br>
    <%= f.text_area :lampiran %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>